{
    "name": "@needle-tools/engine",
    "version": "2.53.0-pre",
    "description": "Needle Engine is a web-based runtime for 3D apps. It runs on your machine for development, and can be deployed anywhere. It is flexible, extensible, and collaboration and XR come naturally.",
    "main": "dist/needle-engine.js",
    "module": "src/needle-engine.ts",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/needle-tools/needle-engine-support"
    },
    "bugs": {
        "url": "https://github.com/needle-tools/needle-engine-support/issues"
    },
    "homepage": "https://needle.tools",
    "author": {
        "name": "Needle",
        "email": "hi@needle.tools",
        "url": "https://needle.tools/"
    },
    "files": [
        "package.json",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md",
        "src",
        "dist",
        "lib"
    ],
    "keywords": [
        "needle",
        "unity",
        "blender",
        "ar",
        "augmented reality",
        "gltf",
        "glb",
        "webar",
        "webvr",
        "webxr",
        "3d"
    ],
    "dependencies": {
        "@dimforge/rapier3d-compat": "^0.9.0",
        "flatbuffers": "2.0.4",
        "md5": "^2.3.0",
        "peerjs": "1.3.2",
        "simplex-noise": "^4.0.1",
        "stats.js": "^0.17.0",
        "three": "npm:@needle-tools/three@^0.146.2",
        "three-mesh-ui": "^6.4.5",
        "three.meshline": "^1.4.0",
        "three.quarks": "^0.7.3",
        "uuid": "^9.0.0",
        "websocket-ts": "^1.1.1"
    },
    "watch": {
        "test:circular-imports": {
            "patterns": [
                "engine/*",
                "engine-components/*",
                "engine-experimental/**/*"
            ],
            "extensions": "ts",
            "quiet": false
        },
        "build:dist": {
            "patterns": [
                "engine/*",
                "engine-components/*",
                "engine-experimental/**/*"
            ],
            "extensions": "ts",
            "quiet": false
        }
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "typings": "lib/needle-engine.d.ts",
    "types": "lib/needle-engine.d.ts"
}